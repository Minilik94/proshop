{
  "name": "proshop",
  "version": "1.0.0",
  "type": "module",
  "description": "e-commerce store built using MERN stack",
  "main": "server.js",
  "scripts": {
    "start": "node ./backend/server.js ",
    "server": "nodemon ./backend/server.js ",
    "client": "npm run dev --prefix frontend",
    "preview": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node ./backend/seeder.js",
    "data:destroy": "node ./backend/seeder.js -d",
    "test": " jest",
    "testJest": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js "
  },
  "author": "Minilik Zeru",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.1"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.0.0"
  }
}
